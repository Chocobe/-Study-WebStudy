<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
	<display-name>pro12</display-name>
	
<!-- JDBC - JNDI -->
	<resource-ref>
		<res-ref-name>jdbc/study/mysql</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>
	
	
<!-- Error Page -->
	<error-page>
		<error-code>404</error-code>
		<location>/err/error404.jsp</location>
	</error-page>
	
	<error-page>
		<error-code>500</error-code>
		<location>/err/error500.jsp</location>
	</error-page>
	
	
<!-- Welcome File -->
	<welcome-file-list>
		<welcome-file>/test02/main.jsp</welcome-file>
		<welcome-file>/test02/add.jsp</welcome-file>
		<welcome-file>/test02/add.html</welcome-file>
	</welcome-file-list>
</web-app>